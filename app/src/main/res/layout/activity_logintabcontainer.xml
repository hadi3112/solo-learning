<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="logintabcontainerVM" type="com.muhammadsapplication.app.modules.logintabcontainer.data.viewmodel.LogintabcontainerVM"/>
    </data>
    <LinearLayout android:id="@+id/linearColumnseekpngOneOne"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyleblue_900">
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true"
            android:gravity="center_horizontal"
            android:id="@+id/scrollViewScrollview"
            android:layout_height="0dp"
            android:layout_marginTop="@dimen/_19pxv"
            android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linearLogin"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearRowwelcome"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_17pxh"
                    android:layout_marginStart="@dimen/_22pxh"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumnwelcome"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtWelcome"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{logintabcontainerVM.logintabcontainerModel.txtWelcome}"
                            style="@style/txtPoppinsbold32" tools:text="@string/lbl_welcome"/>
                        <LinearLayout
                            android:id="@+id/linearRowkindpng15958"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_5pxh"
                            android:layout_marginTop="@dimen/_14pxv"
                            android:layout_width="wrap_content" android:orientation="horizontal">
                            <ImageView
                                android:id="@+id/imageKindpng1595887"
                                android:layout_gravity="bottom"
                                android:layout_height="@dimen/_38pxh"
                                android:layout_marginBottom="@dimen/_6pxv"
                                android:layout_marginTop="@dimen/_21pxv"
                                android:layout_width="@dimen/_38pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_kindpng_1595887"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_kindpng_1595887"/>
                            <LinearLayout android:gravity="start"
                                android:id="@+id/linearColumnpngitem13"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_3pxh"
                                android:layout_width="match_parent" android:orientation="vertical">
                                <LinearLayout
                                    android:id="@+id/linearRowpngitem13119"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent" android:orientation="horizontal">
                                    <ImageView
                                    android:id="@+id/imagePngitem131193"
                                    android:layout_height="@dimen/_25pxh"
                                    android:layout_marginBottom="@dimen/_7pxv"
                                    android:layout_width="@dimen/_25pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_pngitem_131193_1"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_pngitem_131193_1"/>
                                    <TextView android:ellipsize="end"
                                    android:id="@+id/txtC"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_15pxh"
                                    android:layout_marginTop="@dimen/_8pxv"
                                    android:layout_width="wrap_content"
                                    android:singleLine="true"
                                    android:text="@{logintabcontainerVM.logintabcontainerModel.txtC}"
                                    style="@style/txtPoppinsbold16" tools:text="@string/lbl_c"/>
                                </LinearLayout>
                                <ImageView
                                    android:id="@+id/imageKindpng2082370"
                                    android:layout_height="@dimen/_26pxh"
                                    android:layout_marginStart="@dimen/_9pxh"
                                    android:layout_marginTop="@dimen/_8pxv"
                                    android:layout_width="@dimen/_28pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_kindpng_2082370"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_kindpng_2082370"/>
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageSeekpngoneOne"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_122pxh"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:layout_width="@dimen/_77pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_seekpng_1}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_seekpng_1"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearColumnlogin"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_34pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStyleblue_gray_50cornerRadius">
                    <com.google.android.material.tabs.TabLayout
                        android:gravity="center_horizontal"
                        android:id="@+id/tabLayoutTabview"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        app:tabMode="fixed"
                        app:tabSelectedTextColor="@color/blue_900"
                        app:tabTextColor="@color/black_900_01" style="@style/groupStylewhite_1"/>
                    <androidx.viewpager2.widget.ViewPager2
                        android:gravity="center_horizontal"
                        android:id="@+id/viewPagerViewpager"
                        android:layout_height="match_parent"
                        android:layout_weight="1" android:layout_width="match_parent"/>
                </LinearLayout>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>

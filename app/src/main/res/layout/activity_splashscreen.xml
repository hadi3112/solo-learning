<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="splashscreenVM" type="com.muhammadsapplication.app.modules.splashscreen.data.viewmodel.SplashscreenVM"/>
    </data>
    <LinearLayout android:id="@+id/linearSplashScreen"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle">
        <LinearLayout android:gravity="center_vertical"
            android:id="@+id/linearRowtechlineseve"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageView android:id="@+id/imageTechlineseven"
                android:layout_height="@dimen/_69pxh"
                android:layout_marginBottom="@dimen/_3pxv"
                android:layout_width="@dimen/_100pxh"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_techline7}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_techline7"/>
            <ImageView android:id="@+id/imageTelevisionOne"
                android:layout_height="@dimen/_65pxh"
                android:layout_marginStart="@dimen/_179pxh"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_width="@dimen/_81pxh"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_television_blue_900_65x81}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_television_blue_900_65x81"/>
        </LinearLayout>
        <LinearLayout android:gravity="center_vertical"
            android:id="@+id/linearRowtechlinefour"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_7pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageView android:id="@+id/imageTechlinefour"
                android:layout_height="@dimen/_65pxh"
                android:layout_marginTop="@dimen/_28pxv"
                android:layout_width="@dimen/_104pxh"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_techline4}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_techline4"/>
            <ImageView android:id="@+id/imageTechlinesixOne"
                android:layout_height="@dimen/_72pxh"
                android:layout_marginBottom="@dimen/_21pxv"
                android:layout_marginStart="@dimen/_150pxh"
                android:layout_width="@dimen/_106pxh"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_techline6}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_techline6"/>
        </LinearLayout>
        <ImageView android:id="@+id/imageAppiconOne"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_118pxh"
            android:layout_marginTop="@dimen/_22pxv"
            android:layout_width="@dimen/_124pxh"
            android:scaleType="fitXY"
            imageRes="@{@drawable/img_app_icon}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_app_icon"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtSololearning"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_15pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{splashscreenVM.splashscreenModel.txtSololearning}"
            style="@style/txtPoppinsbold24" tools:text="@string/lbl_solo_learning"/>
        <LinearLayout android:gravity="center_vertical"
            android:id="@+id/linearRowtechlineone"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_1pxv"
            android:layout_marginTop="@dimen/_63pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumntechlineo"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_2pxv"
                android:layout_width="wrap_content" android:orientation="vertical">
                <ImageView android:id="@+id/imageTechlineoneOne"
                    android:layout_height="@dimen/_65pxh"
                    android:layout_width="@dimen/_118pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_techline4}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_techline4"/>
                <ImageView android:id="@+id/imageTechlinefive"
                    android:layout_height="@dimen/_48pxh"
                    android:layout_marginTop="@dimen/_20pxv"
                    android:layout_width="@dimen/_44pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_techline7"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_techline7"/>
            </LinearLayout>
            <FrameLayout android:id="@+id/frameStacktelevision"
                android:layout_gravity="bottom"
                android:layout_height="@dimen/_122pxv"
                android:layout_marginTop="@dimen/_13pxv" android:layout_width="@dimen/_141pxh">
                <ImageView android:id="@+id/imageTelevision"
                    android:layout_gravity="top|end"
                    android:layout_height="@dimen/_69pxh"
                    android:layout_width="@dimen/_61pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_techline6}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_techline6"/>
                <ImageView android:id="@+id/imageTechlintwoOne"
                    android:layout_gravity="bottom|center"
                    android:layout_height="@dimen/_65pxv"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_television_blue_900_65x81}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_television_blue_900_65x81"/>
            </FrameLayout>
        </LinearLayout>
    </LinearLayout>
</layout>

<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="gettingStartedTwoVM" type="com.muhammadsapplication.app.modules.gettingstartedtwo.data.viewmodel.GettingStartedTwoVM"/>
    </data>
    <LinearLayout android:id="@+id/linearColumngettingstarted"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:paddingBottom="@dimen/_16pxv"
        android:paddingEnd="@dimen/_14pxh"
        android:paddingStart="@dimen/_14pxh"
        android:paddingTop="@dimen/_16pxv" style="@style/groupStylegray_800">
        <TextView android:ellipsize="end"
            android:gravity="center_horizontal"
            android:id="@+id/txtGettingstarted"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_36pxv"
            android:layout_width="match_parent"
            android:singleLine="true"
            android:text="@{gettingStartedTwoVM.gettingStartedTwoModel.txtGettingstarted}"
            style="@style/txtPoppinsbold24" tools:text="@string/lbl_getting_started"/>
        <FrameLayout android:id="@+id/frameStackmascotimg"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_578pxv"
            android:layout_marginTop="@dimen/_16pxv" android:layout_width="@dimen/_332pxh">
            <LinearLayout android:id="@+id/linearRowspeechbubble"
                android:layout_gravity="bottom|center"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearColumnspeechbub"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_43pxv"
                    android:layout_width="wrap_content" android:orientation="vertical">
                    <FrameLayout android:id="@+id/frameStackspeechbubb"
                        android:layout_height="@dimen/_91pxv" android:layout_width="@dimen/_255pxh">
                        <ImageView android:id="@+id/imageSpeechbubble"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_91pxh"
                            android:layout_width="match_parent"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_speech_bubble"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_speech_bubble"/>
                        <TextView android:gravity="top|center"
                            android:id="@+id/txtHilookingto"
                            android:layout_gravity="top|center"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_12pxv"
                            android:layout_width="match_parent"
                            android:text="@{gettingStartedTwoVM.gettingStartedTwoModel.txtHilookingto}"
                            style="@style/txtPoppinsregular15" tools:text="@string/msg_hi_looking_to_master"/>
                    </FrameLayout>

                    <com.asksira.loopingviewpager.indicator.CustomShapePagerIndicator
                        android:id="@+id/indicatorPagerindicator"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_60pxh"
                        android:layout_marginTop="@dimen/_16pxv" android:layout_width="wrap_content"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageMascotimgOne"
                    android:layout_height="@dimen/_122pxh"
                    android:layout_marginTop="@dimen/_46pxv"
                    android:layout_width="@dimen/_77pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_seekpng_1}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_seekpng_1"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearColumngopremium"
                android:layout_gravity="top|center"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_21pxh"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_width="match_parent"
                android:orientation="vertical"
                android:paddingBottom="@dimen/_17pxv"
                android:paddingEnd="@dimen/_21pxh"
                android:paddingStart="@dimen/_21pxh"
                android:paddingTop="@dimen/_17pxv" style="@style/groupStyleblue_900cornerRadius">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtGopremium"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{gettingStartedTwoVM.gettingStartedTwoModel.txtGopremium}"
                    style="@style/txtPoppinsbold20" tools:text="@string/lbl_go_premium"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnicons8gam"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_66pxv"
                    android:layout_marginStart="@dimen/_3pxh"
                    android:layout_marginTop="@dimen/_30pxv"
                    android:layout_width="wrap_content"
                    android:orientation="vertical" style="@style/groupStylelight_blue_700cornerRadius">
                    <LinearLayout android:id="@+id/linearRowicons8game"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_44pxh"
                        android:layout_marginStart="@dimen/_13pxh"
                        android:layout_marginTop="@dimen/_11pxv"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageIcons8gameOne"
                            android:layout_height="@dimen/_28pxh"
                            android:layout_marginBottom="@dimen/_35pxv"
                            android:layout_width="@dimen/_28pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_icons8_game_controller_64"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_icons8_game_controller_64"/>
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearColumnunlockall"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_6pxh"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_width="match_parent" android:orientation="vertical">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtUnlockall"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_4pxh"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@{gettingStartedTwoVM.gettingStartedTwoModel.txtUnlockall}"
                                style="@style/txtPoppinsbold15" tools:text="@string/msg_unlock_all_levels"/>
                            <TextView android:id="@+id/txtChoosea"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_7pxv"
                                android:layout_width="match_parent"
                                android:text="@{gettingStartedTwoVM.gettingStartedTwoModel.txtChoosea}"
                                style="@style/txtPoppinsregular10" tools:text="@string/msg_choose_a_language"/>
                        </LinearLayout>
                    </LinearLayout>
                    <FrameLayout android:id="@+id/frameStackicons8hack"
                        android:layout_height="@dimen/_172pxv"
                        android:layout_marginTop="@dimen/_13pxv" android:layout_width="@dimen/_245pxh">
                        <LinearLayout
                            android:id="@+id/linearRowicons8hacker"
                            android:layout_gravity="top|center"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_79pxv"
                            android:layout_width="match_parent"
                            android:orientation="horizontal"
                            android:paddingBottom="@dimen/_8pxv"
                            android:paddingEnd="@dimen/_10pxh"
                            android:paddingStart="@dimen/_10pxh"
                            android:paddingTop="@dimen/_8pxv" style="@style/groupStyleblue_A200cornerRadius">
                            <ImageView
                                android:id="@+id/imageIcons8hacker"
                                android:layout_height="@dimen/_31pxh"
                                android:layout_marginBottom="@dimen/_46pxv"
                                android:layout_width="@dimen/_31pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_icons8_hacker_64"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_icons8_hacker_64"/>
                            <LinearLayout
                                android:id="@+id/linearColumnnewchalle"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_14pxv"
                                android:layout_marginStart="@dimen/_9pxh"
                                android:layout_marginTop="@dimen/_2pxv"
                                android:layout_width="wrap_content" android:orientation="vertical">
                                <TextView android:ellipsize="end"
                                    android:gravity="center_horizontal"
                                    android:id="@+id/txtNewchallenge"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:singleLine="true"
                                    android:text="@{gettingStartedTwoVM.gettingStartedTwoModel.txtNewchallenge}"
                                    style="@style/txtPoppinsbold14" tools:text="@string/msg_new_challenge_patterns"/>
                                <TextView
                                    android:gravity="center_horizontal"
                                    android:id="@+id/txtDescription"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_8pxv"
                                    android:layout_width="match_parent"
                                    android:text="@{gettingStartedTwoVM.gettingStartedTwoModel.txtDescription}"
                                    style="@style/txtPoppinsregular10" tools:text="@string/msg_solve_tests_mcqs"/>
                            </LinearLayout>
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/linearRowiconseight"
                            android:layout_gravity="bottom|center"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_86pxv"
                            android:layout_width="match_parent"
                            android:orientation="horizontal"
                            android:paddingHorizontal="@dimen/_8pxh"
                            android:paddingVertical="@dimen/_8pxv" style="@style/groupStylelight_blue_300cornerRadius">
                            <ImageView
                                android:id="@+id/imageIconseightOne"
                                android:layout_height="@dimen/_30pxh"
                                android:layout_marginBottom="@dimen/_37pxv"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_width="@dimen/_30pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_icons8_discounts"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_icons8_discounts"/>
                            <LinearLayout android:gravity="start"
                                android:id="@+id/linearColumndiscounts"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_6pxv"
                                android:layout_marginStart="@dimen/_5pxh"
                                android:layout_width="wrap_content" android:orientation="vertical">
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtDiscounts"
                                    android:layout_height="wrap_content"
                                    android:layout_width="wrap_content"
                                    android:singleLine="true"
                                    android:text="@{gettingStartedTwoVM.gettingStartedTwoModel.txtDiscounts}"
                                    style="@style/txtPoppinsbold15" tools:text="@string/lbl_discounts"/>
                                <TextView
                                    android:id="@+id/txtDescription1"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_4pxh"
                                    android:layout_marginTop="@dimen/_8pxv"
                                    android:layout_width="match_parent"
                                    android:text="@{gettingStartedTwoVM.gettingStartedTwoModel.txtDescription1}"
                                    style="@style/txtPoppinsregular10" tools:text="@string/msg_earn_medals_to_unlock"/>
                            </LinearLayout>
                        </LinearLayout>
                    </FrameLayout>
                </LinearLayout>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
